{
  "root": "build/",
  "clean_urls": false,
  "routes": {
    "/static/*": "/static/",
    "/**": "index.html"
  },
  "proxies": {
    "/graphql/": {
      "origin": "${REACT_APP_API_URL}"
    },
    "/api/": {
      "origin": "https://api.mybitx.com/api/"
    }
  },
  "headers": {
    "/**": {
      "Access-Control-Allow-Origin": "*"
    }
  }
}
